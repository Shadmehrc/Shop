{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Quartz.Core.QuartzSchedulerThread": "Error",
        "Quartz": "Warning"



      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss}]{Message:lj}{Exception} {NewLine}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {SourceContext}:{Level:u3}] {Message:lj}{Exception}{NewLine}",
          "rollingInterval": "Hour"
        }
      }



    ]
  },
